<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidremark">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.chart.cake.ChartActivity" />
        <activity android:name=".ui.payment.ALiPayActivity" />
        <activity android:name=".ui.flow.FlowLayoutActivity" />
        <activity android:name=".ui.contacts.ContactsActivity" />
        <activity android:name=".ui.popup.PopupTestActivity"></activity>
    </application>

</manifest>